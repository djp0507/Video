    <!-- 内边距 -->
    <padding android:left="2dp" android:right="2dp" android:top="2dp" android:bottom="2dp"/>
    <!-- 圆角  radius圆角的弧度 -->
    <corners android:bottomLeftRadius="20dp" android:bottomRightRadius="20dp" android:topLeftRadius="20dp" android:topRightRadius="20dp"></corners>
